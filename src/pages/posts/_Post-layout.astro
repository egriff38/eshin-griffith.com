---
import type { MarkdownContent, MarkdownLayoutProps } from "astro";
import Layout from "../../layouts/Layout.astro";
type Props = MarkdownLayoutProps<{
  title?: string;
}>;
const props = Astro.props;
---

<Layout title={props.frontmatter.title || props.file}>
  <h1>{props.frontmatter.title}</h1>
  <slot />
</Layout>
